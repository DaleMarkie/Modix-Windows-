{
  "name": "modix-panel",
  "version": "1.1.3",
  "private": true,
  "scripts": {
    "dev-frontend": "npx next dev -p 3000",
    "dev-backend": "python3 -m uvicorn backend.api_main:app --host 0.0.0.0 --port 2010 --reload",
    "dev": "concurrently \"npm run dev-frontend\" \"npm run dev-backend\"",
    "build": "NEXT_PUBLIC_SKIP_ESLINT=true npx next build",
    "start-frontend": "npx next start -p 3000",
    "start-backend": "python3 -m uvicorn backend.api_main:app --host 0.0.0.0 --port 2010",
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "package-linux": "npm run build && tar -czvf modix-panel-linux.tar.gz .next package.json package-lock.json node_modules public src backend"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "axios": "^1.12.2",
    "express": "5.1.0",
    "lucide-react": "^0.544.0",
    "next": "14.2.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "@types/react": "19.1.15",
    "autoprefixer": "^10.4.15",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.4",
    "typescript": "5.9.2"
  }
}
