{
  "name": "my-panel",
  "version": "1.1.2",
  "private": true,
  "description": "Modix Panel for managing Project Zomboid servers",
  "main": "main.js",
  "scripts": {
    "preinstall": "node scripts/setupBackend.js",
    "setup-backend": "node scripts/setupBackend.js",
    "dev": "cross-env PORT=3000 API_PORT=2010 concurrently -n FRONTEND,BACKEND,ELECTRON -c cyan,magenta,green \"npx next dev -p 3000\" \"node scripts/startBackend.js\" \"wait-on http://localhost:3000 && electron . --no-sandbox\"",
    "start-all": "node scripts/startAll.js",
    "build": "NEXT_PUBLIC_SKIP_ESLINT=true next build",
    "serve": "next start -p 3000",
    "lint": "next lint",
    "electron-dev": "npm run dev",
    "electron": "electron . --no-sandbox",
    "dist": "electron-builder"
  },
  "dependencies": {
    "next": "^15.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "node-fetch": "^2.7.0",
    "axios": "^1.11.0",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "wait-on": "^7.0.1",
    "electron-is-dev": "^3.0.1",
    "electron": "^26.2.0"
  },
  "devDependencies": {
    "electron-builder": "^24.13.3"
  }
}
