{
  "name": "my-panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "node scripts/setupBackend.js",
    "predev": "npm run setup-backend && npm run gen-import-map",
    "setup-backend": "node scripts/setupBackend.js",
    "dev": "concurrently \"next dev\" \"npm run backend\"",
    "backend": "node -e \"const fs=require('fs');const path=fs.readFileSync('.backendrc','utf8').trim();require('child_process').spawn(path,['backend/api_main.py'],{stdio:'inherit',env:{...process.env,PYTHONPATH:'.'}})\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen-import-map": "node scripts/generateImportMap.js"
  },
  "dependencies": {
    "20": "^3.1.9",
    "@headlessui/react": "^2.2.6",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.534.0",
    "next": "^15.4.5",
    "node": "^24.5.0",
    "react": "^19.1.1",
    "react-d3-tree": "^3.6.6",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-icons": "^3.0.0",
    "@types/react-toastify": "^4.0.2",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
