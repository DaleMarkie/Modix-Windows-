{
  "name": "my-panel",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "preinstall": "node scripts/setupBackend.js",
    "setup-backend": "node scripts/setupBackend.js",
    "predev": "npm run gen-import-map",
    "dev": "npx cross-env PORT=3000 API_PORT=2010 concurrently -n FRONTEND,BACKEND -c cyan,magenta \"npm run next\" \"npm run backend-safe\"",
    "next": "next dev -p 3000",
    "backend": "node scripts/startBackend.js",
    "backend-safe": "node scripts/startBackend.js",
    "backend-install": "py -3 -m pip install -r backend/requirements.txt || python3 -m pip install -r backend/requirements.txt || python -m pip install -r backend/requirements.txt",
    "build:next": "next build && next export",
    "start": "electron .",
    "dist": "npm run build:next && electron-builder",
    "lint": "next lint",
    "gen-import-map": "node scripts/generateImportMap.js",
    "modix-installer": "node scripts/modix-installer.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "inquirer": "^8.2.7",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.534.0",
    "next": "^15.4.5",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-d3-tree": "^3.6.6",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-icons": "^3.0.0",
    "@types/react-toastify": "^4.0.2",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.0"
  },
  "build": {
    "appId": "com.modix.gamepanel",
    "productName": "Modix Game Panel",
    "files": [
      "out/**/*",
      "electron/**/*",
      "backend/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis"
    }
  }
}
